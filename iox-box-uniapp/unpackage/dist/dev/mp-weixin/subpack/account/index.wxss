.f8container {
  border-radius: 40rpx;
}
.f8container .table_head {
  margin-top: 40rpx;
}
.f8container .whiteContainer {
  padding: 20rpx;
}
.f8container .whiteContainer .account {
  display: flex;
  align-items: center;
  margin-bottom: 40rpx;
}
.f8container .whiteContainer .account .left {
  width: 62rpx;
  height: 62rpx;
  margin-right: 20rpx;
}
.f8container .whiteContainer .account .left image {
  width: 100%;
  height: 100%;
}
.f8container .whiteContainer .account .right .top {
  color: #1c1c1e;
}
.f8container .whiteContainer .account .right .bottom {
  color: #636366;
}
.f8container .whiteContainer .account:last-child {
  margin-bottom: 0;
}
.f8container .button {
  position: absolute;
  background-color: #f8f8f8;
  right: 8%;
}
.uni-dialog-title {
  display: flex !important;
}
.uv-form-item__body__right__message {
  margin-left: 78px !important;
}
.uv-button--mini {
  height: 22px !important;
  font-size: 10px !important;
  min-width: 50px !important;
  padding: 0px 8px !important;
  border-radius: 3px !important;
}

