.head_container {
  display: flex;
  padding: 0 40rpx;
  position: relative;
  align-items: center;
}
.head_container .left {
  width: 76rpx;
  height: 76rpx;
}
.head_container .left image {
  width: 100%;
  height: 100%;
}
.head_container .mid {
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
}
.head_container .mid .accountType {
  color: #8e8e93;
  font-size: 26rpx;
}
.head_container .pay {
  position: absolute;
  right: 140rpx;
}
.head_container .notice {
  position: absolute;
  right: 30rpx;
}
.head_container .account {
  display: flex;
  position: absolute;
  right: 30rpx;
}
.head_container .add {
  margin-right: 40rpx;
}
.head_container .img {
  width: 69rpx;
  height: 69rpx;
}
.head_container .img image {
  width: 100%;
  height: 100%;
}

